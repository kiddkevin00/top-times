{
  "name": "top-times-web-core",
  "version": "2.0.1",
  "description": "Core web application for time zone management",
  "main": "./lib/server/",
  "scripts": {
    "-------------- EXPOSED --------------": "",
    "start": "NODE_ENV=production node --stack-size=65500 lib/server/",
    "lint": "npm run lint:scss && npm run lint:js",
    "test": "npm run jest -- --coverage --no-cache",
    "prepare": "rm -rf build/ && NODE_ENV=production webpack -p",
    "prepush": "./bin/no-push-master.sh && npm run lint && npm run test",
    "-------------- INTERNAL -------------": "",
    "lint:js": "eslint --format 'node_modules/eslint-formatter-friendly' --ext .js,.jsx lib/client/app/ lib/server/ spec/",
    "lint:scss": "stylelint 'lib/client/static/assets/styles/**/*.scss'",
    "jest": "NODE_ENV=test BASE_URL=http://localhost jest --config jest.config.js",
    "-------------- LOCAL DEV ------------": "",
    "dev": "NODE_ENV=development webpack-dev-server",
    "test:watch": "npm run jest -- --bail --watch",
    "test:update": "npm run jest -- --no-cache -u"
  },
  "config": {
    "ip": "0.0.0.0",
    "port": 8088
  },
