{
  "presets": [ ["es2015", { "modules": false }], "es2016", "es2017", "react", "env"],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    [
      "babel-plugin-transform-builtin-extend",
      {
        "globals": ["Error", "Array"]
      },
    ]
  ]
}